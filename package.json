{
  "name": "nitro-ui",
  "version": "0.0.1",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "private": true,
  "scripts": {
    "bc": "rollup --config rollup.config.ts --configPlugin typescript",
    "lint": "next lint",
    "format": "prettier \"**/*.+(js|jsx|ts|tsx|json|css|md)\" --config package.json --write",
    "format:check": "prettier \"**/*.+(js|jsx|ts|tsx|json|css|md)\" --config package.json --check",
    "storybook": "start-storybook -p 6006 -s public",
    "build": "build-storybook",
    "build-storybook": "build-storybook -s public",
    "serve-storybook": "serve storybook-static"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.3",
    "@heroicons/react": "^2.0.12",
    "class-variance-authority": "^0.2.4",
    "next": "latest",
    "sass": "^1.49.0"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@rollup/plugin-typescript": "^9.0.2",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/builder-webpack5": "^6.4.9",
    "@storybook/manager-webpack5": "^6.4.9",
    "@storybook/react": "^6.4.9",
    "@swc/cli": "^0.1.57",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "18.0.21",
    "autoprefixer": "^10.4.12",
    "babel-loader": "^8.2.3",
    "eslint": "8.26.0",
    "eslint-config-next": "13.0.1",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollup": "^3.2.5",
    "serve": "13.0.2",
    "storybook-addon-next": "1.3.1",
    "storybook-tailwind-dark-mode": "^1.0.15",
    "tailwindcss": "^3.1.8",
    "typescript": "4.5.5"
  }
}
