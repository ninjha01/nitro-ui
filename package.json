{
  "name": "@ninjha01/nitro-ui",
  "private": false,
  "version": "0.0.12",
  "scripts": {
    "build": "tsc && vite build",
    "dev": "yarn storybook",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:fix": "prettier --check --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "prepack": "json -f package.json -I -e \"delete this.devDependencies; delete this.dependencies\""
  },
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/nitro-ui.umd.js",
  "module": "./dist/nitro-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nitro-ui.es.js",
      "require": "./dist/nitro-ui.umd.js"
    }
  },
  "description": "collection of ui components",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ninjha01/storybook.git"
  },
  "keywords": [
    "ui",
    "components"
  ],
  "author": "Nishant Jha",
  "bugs": {
    "url": "https://github.com/ninjha01/storybook/issues"
  },
  "homepage": "https://github.com/ninjha01/storybook#readme"
}
