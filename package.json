{
  "name": "@ninjha01/nitro-ui",
  "private": false,
  "version": "0.1.62",
  "scripts": {
    "build": "pnpm lint && vite build && pnpm build-css",
    "build-css": "npx tailwindcss -o ./nitro.css --minify && mv ./nitro.css ./dist/nitro.css",
    "dev": "pnpm storybook dev -p 6006",
    "build-storybook": "pnpm build && storybook build",
    "format": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\" --config package.json",
    "format:fix": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\" --config package.json",
    "lint": "tsc && eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "lint:watch": "nodemon --watch src --ext js,jsx,ts,tsx --exec pnpm lint",
    "prepublishOnly": "pnpm lint && pnpm format && pnpm build",
    "build:watch": "nodemon --watch src --ext js,jsx,ts,tsx,json,css,scss,html --exec pnpm build",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.18",
    "@heroicons/react": "^2.1.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@rdkit/rdkit": "2023.9.2-1.0.0",
    "@tanstack/react-table": "^8.16.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cmdk": "^1.0.0",
    "daisyui": "^4.7.3",
    "genbank-parser": "^1.2.4",
    "molstar": "^4.0.1",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.51.0",
    "react-markdown": "^9.0.1",
    "react-render-if-visible": "^2.1.1",
    "react-selectable": "^2.1.1",
    "remark-gfm": "^4.0.0",
    "storybook-addon-performance": "^0.17.3",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/testing-library": "^0.2.2",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/plotly.js-basic-dist-min": "^2.12.4",
    "@types/react": "^18.2.65",
    "@types/react-dom": "^18.2.22",
    "@types/react-plotly.js": "^2.6.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "babel-loader": "^9.1.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-storybook": "^0.8.0",
    "jsdom": "^24.0.0",
    "json": "^11.0.0",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.12",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.17",
    "storybook-tailwind-dark-mode": "^1.0.22",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6",
    "vite-plugin-dts": "^3.7.3",
    "vitest": "^1.3.1"
  },
  "license": "UNLICENSED",
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/nitro-ui.umd.js",
  "module": "./dist/nitro-ui.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/nitro-ui.es.js",
      "require": "./dist/nitro-ui.umd.js",
      "types": "./dist/index.d.ts"
    }
  },
  "description": "collection of ui components",
  "keywords": [
    "ui",
    "components"
  ],
  "author": "Nishant Jha",
  "prettier": {
    "plugins": [
      "prettier-plugin-tailwindcss"
    ]
  }
}
